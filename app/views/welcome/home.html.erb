<%# Add the Facebook login link here %>
<% if session[:user_id] %>
    <h1>Name: <%= @user.name %></h1>
    <h2>Email: <%= @user.email %></h2>
    <h2>Facebook UID: <%= @user.uid %></h2>
    <img src="<%= @user.image %>">
    <%= link_to 'Logout', logout_path, method: :delete %>
<% else %>
    <%= link_to 'Login with Facebook!', '/auth/facebook' %>
<% end %>

